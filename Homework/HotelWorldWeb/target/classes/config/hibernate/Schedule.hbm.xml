<hibernate-mapping package="com.hotelworld.entity">
    <class name="Schedule" table="schedule">
        <id name="id" column="s_id"/>
        <property name="hotelId" column="s_hotel_id"/>
        <property name="date" column="s_date"/>
        <property name="priceSingle" column="s_single"/>
        <property name="priceStandard" column="s_standard"/>
        <property name="priceDouble" column="s_double"/>
        <property name="priceSuit" column="s_suit"/>
        <property name="isDefault" column="s_is_default"/>
    </class>
</hibernate-mapping>